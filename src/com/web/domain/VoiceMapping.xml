<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.web.dao.VoiceDao">
	
	<insert id="saveVoice" parameterType="voice">
	insert into voiceD(
            name,tel,email,voiceTitle,voiceContent
		)
		values
		(
		#{name},#{tel},#{email},#{voiceTitle},#{voiceContent}
		)
	</insert>
 	
 	
</mapper>
