<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.web.dao.ExchangeDao">
	<insert id="saveExchange" parameterType="Exchange">
		insert into exchangeD (usId,integralId,exchangeTime,exchangeState)values(#{usId},#{integralId},#{exchangeTime},#{exchangeState})
	</insert>
 
</mapper>
