<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.web.dao.TransVerifyDao">
	 
	 <insert id="saveTransVerify" parameterType="TransVerify">
	  insert into transVerifyD (realName,IdCard,frontUrl,reverseUrl,translatorId)
	  values(#{realName},#{IdCard},#{frontUrl},#{reverseUrl},#{translatorId})  
	 </insert>
 
</mapper>
